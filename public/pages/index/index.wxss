/*由 src/less/pages/index/index.less 生成，请勿手动修改以免被覆盖！*/
page {
  padding: 0;
  margin: 0;
  width: 100%;
}
.activity_page {
  min-height: 100vh;
  width: 100%;
  padding: 0rpx;
  margin: 0rpx;
  padding-bottom: 300rpx;
  position: relative;
}
.activity_page .header_banner {
  width: 100%;
}
.activity_page .header_banner .img {
  width: 100%;
}
.activity_page .mine {
  position: absolute;
  top: 100rpx;
  right: 0rpx;
  background: rgba(17, 17, 17, 0.43);
  border: 2rpx solid rgba(7, 7, 7, 0.11);
  border-top-left-radius: 45rpx;
  border-bottom-left-radius: 45rpx;
  width: 147rpx;
  height: 40rpx;
  display: flex;
  align-items: center;
  padding-left: 5rpx;
  box-sizing: border-box;
}
.activity_page .mine .mine_icon {
  width: 30rpx;
  height: 30rpx;
  background-image: url('data:image/png;base64,');
  background-size: contain;
  background-repeat: no-repeat;
}
.activity_page .mine .mine_title {
  flex: 1;
  color: #fff;
  font-size: 20rpx;
  text-align: center;
}
.activity_page .mine .mine_title button {
  display: unset;
  width: 100%;
  background-color: transparent;
  font-size: 20rpx;
  text-align: center;
  color: #fff;
  line-height: 40rpx;
  padding: 0;
}
.noscroll {
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100%;
  overflow: hidden;
  position: fixed;
  z-index: 0;
}
